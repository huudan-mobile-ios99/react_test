{
    "version": 2,
    "builds": [
      {
        "src": "api/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "dist/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/server.js"
      },
      {
        "src": "/images/(.*)",
        "dest": "/public/images/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/$1"
      }
    ]
  }
